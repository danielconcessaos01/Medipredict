{
  "version": 2,
  "builds": [
    {
      "src": "api/multi_disease_api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/multi_disease_api.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11.0"
  }
}